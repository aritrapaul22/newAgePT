          <JSR223Timer guiclass="TestBeanGUI" testclass="JSR223Timer" testname="Pacing Timer" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">String startTime = vars.get(&quot;pacingStartTime&quot;)
Long executionTime = new Date().getTime() - Long.parseLong(startTime)

log.info(&quot;Execution Time : &quot; + executionTime);

Long pacing = 60000 - executionTime;
log.info(&quot;Pacing : &quot; + pacing);

if(pacing &gt; 0){
	Integer iPacing = pacing != null ? (pacing/2).intValue() : null;
	vars.put(&quot;myDelay&quot;, String.valueOf(iPacing));
	return 0;
} else{
	vars.put(&quot;myDelay&quot;, 0);
	return 1;
}
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223Timer>
